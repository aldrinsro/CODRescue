{
  "version": 2,
  "builds": [
    {
      "src": "config/wsgi_vercel.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/media/(.*)",
      "destination": "/media/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/config/wsgi_vercel.py"
    }
  ],
  "functions": {
    "config/wsgi_vercel.py": {
      "excludeFiles": "{static/vendor/fontawesome-free-5.15.4-web/svgs,static/vendor/fontawesome-free-5.15.4-web/webfonts,static/vendor/fontawesome-free-5.15.4-web/less,static/vendor/fontawesome-free-5.15.4-web/scss,static/vendor/fontawesome-free-5.15.4-web/sprites,static/vendor/fontawesome-free-5.15.4-web/metadata,__pycache__,*.pyc,*.log,test_*.py,*_test.py,*.tmp,*.temp}/**"
    }
  },
  "env": {
    "DJANGO_SETTINGS_MODULE": "config.settings"
  }
} 