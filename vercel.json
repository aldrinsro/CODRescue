{
  "version": 2,
  "builds": [
    {
      "src": "config/wsgi_vercel.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/media/(.*)",
      "destination": "/media/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/config/wsgi_vercel.py"
    }
  ],
  "functions": {
    "config/wsgi_vercel.py": {
      "excludeFiles": "static/vendor/**"
    }
  },
  "env": {
    "DJANGO_SETTINGS_MODULE": "config.settings"
  }
} 